name: RaidRush
short: L:RaidRush
web: "http://save.raidrush.ws/"
uri: qr{save\.raidrush\.ws/}
slots: max
status: BROKEN 2010-06-04

start:
	GET( $-{_uri} );

	my @list;
	push @list, "/404.php.php?id=$1&amp;key=$2"
		while s/onclick="get\('(.*?)','FREE','(.*?)'\)//;

	ERROR("no links") unless @list;

	$-{list} = \@list;
	$-{outlist} = [];

stage_get_link:
	GET( shift @{$-{list}} );
	push @{$-{outlist}}, "http://" . $_;

	GOTO stage_get_link if @{$-{list}};

	LINK( @{$-{outlist}} );

