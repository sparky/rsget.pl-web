name: AnonymTo
short: L:AnonymTo
web: "http://anonym.to/"
uri: qr{anonym\.to/}
slots: max
status: OK 2010-04-03

unify:
	return $_;

start:
	GET( $-{_uri} );
	$_ = $-{_uri};
	s{^http://.*?http}{http};
	LINK( $_ );

