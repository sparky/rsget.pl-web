name: RSProtect
short: L:RSProtect
web: "http://www.rsprotect.com/"
uri: qr{rsprotect\.com/rc-.*?/}
slots: max
status: OK 2010-07-12

unify:
	s/#.*//;

start:
	GET( $-{_uri} );

	! m{&lt;form method="post" onsubmit="return ch.*?" action="(.*?)"};
	$_ = $1;
	s/&amp;#x(..);/chr hex $1/eg;

	LINK( $_ );

# vim: filetype=perl:ts=4:sw=4
</pre>
